{
  "name": "bombo-slack-ts",
  "version": "1.0.0",
  "description": "A Slack bot that generates custom Bombo character stickers using AI image generation - TypeScript version",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "dev:watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": ["slack", "bot", "ai", "typescript", "bombo"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.17.1",
    "dotenv": "^16.3.1",
    "replicate": "^1.0.1",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
