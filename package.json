{
  "name": "bombo-slack-ts",
  "version": "1.0.0",
  "description": "A Slack bot that generates custom Bombo character stickers using AI image generation - TypeScript version",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "dev:watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": [
    "slack",
    "bot",
    "ai",
    "typescript",
    "bombo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.17.1",
    "dotenv": "^16.3.1",
    "minio": "^8.0.5",
    "openai": "^4.20.1",
    "replicate": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
